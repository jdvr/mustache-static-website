{
  "name": "mustache-static-website",
  "version": "0.0.1",
  "description": " A lightweight way to generate flexible static websites. Mustache and Node.",
  "main": "generate.js",
  "watch": {
    "generate": {
      "patterns": [
        "tpl",
        "generable-pages"
      ],
      "extensions": "json,mustache,js",
      "quiet": false
    }
  },
  "scripts": {
    "generate": "./generate.js",
    "watch": "npm-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jdvr/mustache-static-website.git"
  },
  "keywords": [
    "mustache",
    "node",
    "npm",
    "static",
    "website"
  ],
  "author": "jdvr",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/jdvr/mustache-static-website/issues"
  },
  "homepage": "https://github.com/jdvr/mustache-static-website#readme",
  "devDependencies": {
    "buildify": "^0.4.0",
    "mustache": "^2.2.1",
    "npm-watch": "^0.1.6"
  }
}
